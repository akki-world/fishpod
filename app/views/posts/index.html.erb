<div class="posts-title">
  <h2 class="mt-3 pl-3">POSTS</h2>
</div>

<%= render "search_form" %>

<div class="container">
  <% @posts.each do |post| %>
    <div class="box">
      <%= link_to image_tag(post.image.to_s), post_path(post) %>
    </div>
  <% end %>

  <div class="my-3 pagenate">
    <%= paginate @posts %>
    <%= page_entries_info @posts %>
  </div>
</div>
