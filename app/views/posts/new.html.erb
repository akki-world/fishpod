<h1>Posts#new</h1>

<%= form_with(model: @post, locale: true) do |f| %>
  <%= f.label :photo %>
  <%= f.file_field :image %>

  <%= f.label :size %>
  <%= f.text_field :size %>

  <%= f.label :lure %>
  <%= f.text_field :lure %>

  <%= f.label :commetn %>
  <%= f.text_area :size %>

  <%= f.submit %>
<% end %>
